{
  "name": "Project LRP",
  "description": "Latency Reducing Chrome Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "service_worker": "scripts/background.js"
  },
  "permissions": [
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}
